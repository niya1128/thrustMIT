int buzzPin=6,dt1=1,dt2=2;
int i;
void setup()
{
  pinMode(buzzPin, OUTPUT);
}

void loop()
{
  for(i=0;i<100;i++)
  {
    digitalWrite(buzzPin,HIGH);
    delay(dt1);
    digitalWrite(buzzPin,LOW);
    delay(dt1);
  }
  for(i=0;i<100;i++)
  {
   digitalWrite(buzzPin,HIGH);
   delay(dt2);
   digitalWrite(buzzPin,LOW);
   delay(dt2);
  }
}
